user nginx;
worker_processes auto;

error_log  /var/log/nginx/error.log;
pid        /var/run/nginx.pid;

load_module modules/ngx_http_js_module.so;
load_module modules/ngx_stream_js_module.so;

events {
  worker_connections  1024;
}

http {
  include       /etc/nginx/mime.types;
  default_type  application/octet-stream;

  js_include maskQueryParams.js;
  js_set $request_uri_masked maskQueryParams;

  log_format main <%= log_format %>;

  access_log /var/log/nginx/access.log  main;

  sendfile on;
  tcp_nopush on;
  tcp_nodelay on;

  # allow large file upload
  client_max_body_size 10m;

  gzip  on;
  gzip_static  on;
  gzip_http_version 1.0;
  gzip_comp_level 2;
  gzip_proxied any;
  gzip_types application/x-javascript application/xhtml+xml application/xml application/xml+rss text/css text/javascript text/plain text/xml;
  gzip_vary on;
  gzip_disable "MSIE [1-6].(?!.*SV1)";

  keepalive_timeout  5;
  server_tokens off;

  upstream <%= upstream_name %> {
    server <%= upstream_name %>:<%= upstream_port %>;
  }

  include /etc/nginx/conf.d/*.conf;
}
